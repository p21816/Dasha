<Window x:Class="FileDataBase.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="440" Width="300">
    <DockPanel>
        <StackPanel>
            <Label Content="Найти контакт:"  Margin="10 5"></Label>
            <TextBox Margin="10 5" TextChanged="TextBox_TextChanged"/>
            <ListBox Height="70" Margin="10 5"></ListBox>
            <Label Content="Добавить контакт"  Margin="10 3" HorizontalAlignment="Center"></Label>
            <Label Content="Имя:"  Margin="10 3"></Label>
            <TextBox Margin="10 3"></TextBox>
            <Label Content="Фамилия:"  Margin="10 3"></Label>
            <TextBox Margin="10 3"></TextBox>
            <Label Content="ID:"  Margin="10 3"></Label>
            <TextBox Margin="10 3"></TextBox>
            <Button Content="Добавить"  Margin="10 5"></Button>
        </StackPanel>
        
    </DockPanel>
</Window>
