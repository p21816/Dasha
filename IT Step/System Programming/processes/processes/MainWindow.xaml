<Window x:Class="processes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <DataGrid x:Name="dg" AutoGenerateColumns="True">
        </DataGrid>
        <WrapPanel>
          <Button Content="New" Width="30" Click="Button_Click"></Button>
          <Button Content="Kill" Width="30" Click="Button_Click_1"></Button>
          <Button Content="+" Width="30"></Button>
          <Button Content="-" Width="30"></Button>
        </WrapPanel>
    </StackPanel>
</Window>
